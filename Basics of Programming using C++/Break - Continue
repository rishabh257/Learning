#include <iostream>
using namespace std;

int main()
{
   for (int i=0; i<7; i++)
   {
       cout << "Value of i is " << i << endl;
       cout << "Looping...." << endl;
       
       if (i == 3)
       {
       cout << "Program stopped. Loop Quitting...." << endl;
       continue;
       }
   }
}
